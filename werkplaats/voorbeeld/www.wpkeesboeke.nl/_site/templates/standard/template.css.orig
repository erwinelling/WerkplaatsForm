.qanda > .box {
  margin-top: 0.5em;
}
.qanda:not([class*='style-']),
.qanda.style-triviant {
  -webkit-transform: translateZ(100px);
  transform: translateZ(100px);
  -webkit-perspective: 500;
  perspective: 500;
}
.qanda:not([class*='style-']) > .box,
.qanda.style-triviant > .box {
  box-sizing: border-box;
  width: 216px;
  height: 159px;
  -webkit-transform-style: preserve-3d;
  transform-style: preserve-3d;
}
.qanda:not([class*='style-']) > .box .qanda-q,
.qanda.style-triviant > .box .qanda-q,
.qanda:not([class*='style-']) > .box .qanda-a,
.qanda.style-triviant > .box .qanda-a {
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  box-sizing: border-box;
  position: absolute;
  z-index: 1;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  transition: -webkit-transform 0.7s ease;
  transition: transform 0.7s ease;
  font-family: 'Lucida Grande', 'Helvetica Neue', 'Helvetica', 'Arial';
  font-size: 12px;
  color: #2B1B23;
}
.qanda:not([class*='style-']) > .box .qanda-q,
.qanda.style-triviant > .box .qanda-q {
  background: url(../../htmlcs/qanda/doodle-vraag.png);
  padding: 25px 30px 20px 55px;
}
.qanda:not([class*='style-']) > .box .qanda-a,
.qanda.style-triviant > .box .qanda-a {
  background: url(../../htmlcs/qanda/doodle-antwoord.png);
  padding: 25px 20px 25px 100px;
}
.qanda:not([class*='style-']) > .box p,
.qanda.style-triviant > .box p {
  margin: 0;
}
.qanda .qanda-q {
  -webkit-transform: rotateY(0deg);
  transform: rotateY(0deg);
}
.qanda .qanda-a {
  -webkit-transform: rotateY(180deg);
  transform: rotateY(180deg);
}
.qanda.selected .qanda-q {
  -webkit-transform: rotateY(180deg);
  transform: rotateY(180deg);
}
.qanda.selected .qanda-a {
  -webkit-transform: rotateY(360deg);
  transform: rotateY(360deg);
}
.isOpera .qanda .qanda-q,
.isOldIE .qanda .qanda-q {
  display: block;
}
.isOpera .qanda .qanda-a,
.isOldIE .qanda .qanda-a {
  display: none;
}
.isOpera .qanda.selected .qanda-q,
.isOldIE .qanda.selected .qanda-q {
  display: none;
}
.isOpera .qanda.selected .qanda-a,
.isOldIE .qanda.selected .qanda-a {
  display: block;
}
.reminder > .box {
  padding: 30px;
  color: #D40E0E;
}
.reminder > .box:before {
  content: '';
  box-sizing: border-box;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
