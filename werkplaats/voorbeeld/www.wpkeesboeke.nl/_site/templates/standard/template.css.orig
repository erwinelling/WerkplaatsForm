.slideshow {
  z-index: 1000;
}
.slideshow .box {
  margin: 25px 25px;
}
.slideshow .box .poster {
  position: relative;
  z-index: 20;
  box-shadow: 0 0 4px rgba(0, 0, 0, 0.35);
  border-radius: 3px;
  background-clip: padding-box;
  background: white;
  padding: 12px 12px 0 12px;
  transition: all 0.5s 0.4s;
}
.slideshow .box .poster .caption {
  box-sizing: border-box;
  display: block;
  position: relative;
  z-index: 30;
  padding: 10px;
  text-align: center;
}
.slideshow .box .poster:before {
  content: '';
  box-sizing: border-box;
  display: block;
  position: absolute;
  top: 40%;
  left: 50%;
  width: 70px;
  height: 67px;
  margin-left: -35px;
  background: url(../../htmlcs/slideshow/play.png);
  padding-left: 14px;
  font-size: 28px;
  line-height: 46px;
  color: white;
  transition: all 1s;
}
.slideshow .box .fakeposter {
  box-sizing: border-box;
  position: absolute;
  z-index: 10;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  box-shadow: 0 0 4px rgba(0, 0, 0, 0.35);
  border-radius: 3px;
  background-clip: padding-box;
  background: white;
  padding: 12px 12px 0 12px;
  transition: opacity 0.4s;
}
.slideshow .box .fakeposter:nth-of-type(1) {
  -webkit-transform: rotate(8deg);
  transform: rotate(8deg);
}
.slideshow .box .fakeposter:nth-of-type(2) {
  -webkit-transform: rotate(6deg);
  transform: rotate(6deg);
}
.slideshow .box .fakeposter:nth-of-type(3) {
  -webkit-transform: rotate(4deg);
  transform: rotate(4deg);
}
.slideshow .box .fakeposter:nth-of-type(4) {
  -webkit-transform: rotate(2deg);
  transform: rotate(2deg);
}
.slideshow .box .slides {
  position: absolute;
  z-index: 10;
  top: 0px;
  transition: opacity 0.8s;
}
.slideshow .box .slides .slide {
  display: block;
  position: absolute;
}
.slideshow .box .slides .slide img {
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);
  border-radius: 3px;
  background-clip: padding-box;
  background: white;
  padding: 12px;
}
.slideshow:not(.playing) .poster {
  opacity: 1;
  -webkit-transform: scale(1);
  transform: scale(1);
}
.slideshow:not(.playing) .poster:before {
  opacity: 1;
  -webkit-transform: scale(1);
  transform: scale(1);
}
.slideshow:not(.playing) .fakeposter {
  opacity: 1;
}
.slideshow:not(.playing) .slides {
  opacity: 0;
}
.slideshow.playing .poster {
  opacity: 0;
  -webkit-transform: scale(2);
  transform: scale(2);
}
.slideshow.playing .poster:before {
  opacity: 0;
  -webkit-transform: scale(5);
  transform: scale(5);
}
.slideshow.playing .fakeposter {
  opacity: 0;
}
.slideshow.playing .slides {
  opacity: 1;
}
