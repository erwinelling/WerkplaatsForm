#fxs.fx-leaves.leaves-hidden {
  opacity: 0;
  -webkit-animation: leaves-hidden 1.5s forwards;
  animation: leaves-hidden 1.5s forwards;
}
@-webkit-keyframes leaves-hidden {
  0% {
    display: block;
    top: 0px;
    opacity: 1;
  }
  100% {
    display: none;
    top: 30px;
    opacity: 0;
  }
}
@keyframes leaves-hidden {
  0% {
    display: block;
    top: 0px;
    opacity: 1;
  }
  100% {
    display: none;
    top: 30px;
    opacity: 0;
  }
}
#fxs.fx-leaves > .leaves-leaf {
  position: absolute;
  xvisibility: hidden;
  top: -150px;
  width: 110px;
  height: 110px;
  margin-top: -65px;
  -webkit-animation-iteration-count: 2, 1;
  -webkit-animation-direction: alternate, normal;
  -webkit-animation-timing-function: ease-in-out, ease-in;
  -webkit-transform-origin: 50% -100%, 0% 0%;
  -webkit-animation-fill-mode: forwards;
  animation-iteration-count: 2, 1;
  animation-direction: alternate, normal;
  animation-timing-function: ease-in-out, ease-in;
  transform-origin: 50% -100%, 0% 0%;
  animation-fill-mode: forwards;
}
#fxs.fx-leaves > .leaves-leaf.leaves-leaf1 {
  background-image: url(../../fxs/leaves/realLeaf1.png);
}
#fxs.fx-leaves > .leaves-leaf.leaves-leaf2 {
  background-image: url(../../fxs/leaves/realLeaf2.png);
}
#fxs.fx-leaves > .leaves-leaf.leaves-leaf3 {
  background-image: url(../../fxs/leaves/realLeaf3.png);
}
#fxs.fx-leaves > .leaves-leaf.leaves-leaf4 {
  background-image: url(../../fxs/leaves/realLeaf4.png);
}
/* Makes a leaf fall */
@-webkit-keyframes leaves-drop {
  0% {
    visibility: hidden;
    top: -10%;
  }
  1% {
    visibility: visible;
    top: -10%;
  }
  100% {
    visibility: visible;
    top: 100%;
  }
}
@keyframes leaves-drop {
  0% {
    top: -10%;
  }
  100% {
    top: 100%;
  }
}
/* Rotate a leaf clockwise */
@-webkit-keyframes leaves-clockwiseSpin {
  0% {
    -webkit-transform: translate(-150px, 0) rotate(-50deg);
  }
  100% {
    -webkit-transform: translate(150px, 0) rotate(50deg);
  }
}
@keyframes leaves-clockwiseSpin {
  0% {
    transform: translate(-150px, 0) rotate(-50deg);
  }
  100% {
    transform: translate(150px, 0) rotate(50deg);
  }
}
/* Flips a leaf and rotates it counterclockwise */
@-webkit-keyframes leaves-counterclockwiseSpin {
  0% {
    -webkit-transform: translate(150px, 0) scale(-1, 1) rotate(50deg);
  }
  100% {
    -webkit-transform: translate(-150px, 0) scale(-1, 1) rotate(-50deg);
  }
}
@keyframes leaves-counterclockwiseSpin {
  0% {
    transform: translate(150px, 0) scale(-1, 1) rotate(50deg);
  }
  100% {
    transform: translate(-150px, 0) scale(-1, 1) rotate(-50deg);
  }
}
#fxs.fx-snow {
  position: fixed;
  z-index: 0;
  top: -50px;
  left: 0;
  width: 100%;
  height: 1px;
  text-align: center;
}
#fxs.fx-snow i {
  display: inline-block;
  visibility: hidden;
  height: 16px;
  width: 16px;
  border-radius: 50%;
  -webkit-transform-origin: left -20px;
  -webkit-animation-name: snowflakes-falling;
  -webkit-animation-duration: 3s;
  -webkit-animation-direction: normal;
  -webkit-animation-iteration-count: 20;
  -webkit-animation-timing-function: linear;
  -webkit-animation-delay: 2s;
  -webkit-animation-fill-mode: forwards;
  -webkit-animation-play-state: running;
  transform-origin: left -20px;
  animation-name: snowflakes-falling;
  animation-duration: 3s;
  animation-direction: normal;
  animation-iteration-count: 20;
  animation-timing-function: linear;
  animation-delay: 2s;
  animation-fill-mode: forwards;
  animation-play-state: running;
  background-image: linear-gradient(180deg, rgba(255, 255, 255, 0) 40%, #fff 40%, #eee 60%, rgba(255, 255, 255, 0) 60%), linear-gradient(90deg, rgba(255, 255, 255, 0) 40%, #fff 40%, #eee 60%, rgba(255, 255, 255, 0) 60%), linear-gradient(45deg, rgba(255, 255, 255, 0) 43%, #fff 43%, #eee 57%, rgba(255, 255, 255, 0) 57%), linear-gradient(135deg, rgba(255, 255, 255, 0) 43%, #fff 43%, #eee 57%, rgba(255, 255, 255, 0) 57%);
}
/* animation used for ALL snowflakes */
@-webkit-keyframes snowflakes-falling {
  0% {
    visibility: visible;
    -webkit-transform: translate3d(0, 0, 0) rotate(0deg) scale(0.9, 0.9);
  }
  100% {
    visibility: visible;
    -webkit-transform: translate3d(30px, 1500px, 0) rotate(360deg) scale(1.1, 1.1);
  }
}
@keyframes snowflakes-falling {
  0% {
    visibility: visible;
    transform: translate3d(0, 0, 0) rotate(0deg) scale(0.9, 0.9);
  }
  100% {
    visibility: visible;
    transform: translate3d(30px, 1500px, 0) rotate(360deg) scale(1.1, 1.1);
  }
}
/* different sizes, and default origins based on size */
#fxs.fx-snow i:nth-of-type(4n) {
  height: 30px;
  width: 30px;
  -webkit-transform-origin: right -30px;
  transform-origin: right -30px;
}
#fxs.fx-snow i:nth-of-type(4n+1) {
  height: 24px;
  width: 24px;
  -webkit-transform-origin: left -30px;
  transform-origin: left -30px;
}
#fxs.fx-snow i:nth-of-type(4n+2) {
  height: 10px;
  width: 10px;
  -webkit-transform-origin: -30px 0;
  transform-origin: -30px 0;
}
/* different speeds, with iteration counts to equal 3minutes approx. */
#fxs.fx-snow i:nth-of-type(4n) {
  -webkit-animation-duration: 5.3s;
  -webkit-animation-iteration-count: 12;
  -webkit-transform-origin: -10px -20px;
  animation-duration: 5.3s;
  animation-iteration-count: 12;
  transform-origin: -10px -20px;
}
#fxs.fx-snow i:nth-of-type(4n+1) {
  -webkit-animation-duration: 3.1s;
  -webkit-animation-iteration-count: 20;
  -webkit-transform-origin: 10px -20px;
  animation-duration: 3.1s;
  animation-iteration-count: 20;
  transform-origin: 10px -20px;
}
#fxs.fx-snow i:nth-of-type(4n+2) {
  -webkit-animation-duration: 1.7s;
  -webkit-animation-iteration-count: 35;
  -webkit-transform-origin: right -20px;
  animation-duration: 1.7s;
  animation-iteration-count: 35;
  transform-origin: right -20px;
}
/* different delays so they don't all start at the same time */
#fxs.fx-snow i:nth-of-type(3n) {
  -webkit-animation-delay: 2.3s;
  animation-delay: 2.3s;
}
#fxs.fx-snow i:nth-of-type(3n+1) {
  -webkit-animation-delay: 1.5s;
  animation-delay: 1.5s;
}
#fxs.fx-snow i:nth-of-type(3n+2) {
  -webkit-animation-delay: 3.4s;
  animation-delay: 3.4s;
}
/* playing with timing functions to make it a teeny bit more random like */
#fxs.fx-snow i:nth-of-type(5n) {
  -webkit-animation-timing-function: ease-in-out;
  animation-timing-function: ease-in-out;
}
#fxs.fx-snow i:nth-of-type(5n+1) {
  -webkit-animation-timing-function: ease-out;
  animation-timing-function: ease-out;
}
#fxs.fx-snow i:nth-of-type(5n+2) {
  -webkit-animation-timing-function: ease;
  animation-timing-function: ease;
}
#fxs.fx-snow i:nth-of-type(5n+3) {
  -webkit-animation-timing-function: ease-in;
  animation-timing-function: ease-in;
}
#fxs.fx-snow i:nth-of-type(5n+4) {
  -webkit-animation-timing-function: linear;
  animation-timing-function: linear;
}
#fxs.fx-snow i:nth-of-type(11n) {
  -webkit-animation-timing-function: cubic-bezier(0.2, 0.3, 0.8, 0.9);
  animation-timing-function: cubic-bezier(0.2, 0.3, 0.8, 0.9);
}
/* playing with opacity to make it a teeny bit more random like */
#fxs.fx-snow i:nth-of-type(7n) {
  opacity: 0.5;
}
#fxs.fx-snow i:nth-of-type(7n+2) {
  opacity: 0.3;
}
#fxs.fx-snow i:nth-of-type(7n+4) {
  opacity: 0.7;
}
#fxs.fx-snow i:nth-of-type(7n+6) {
  opacity: 0.6;
  -webkit-animation-timing-function: ease-in;
  -webkit-transform-origin: left 10px;
  animation-timing-function: ease-in;
  transform-origin: left 10px;
}
#fxs.fx-snow i:nth-of-type(7n+1) {
  opacity: 0.8;
}
