  }
  100% {
    box-shadow: 0 0 15px rgba(0, 0, 0, 0.5), inset 0 0 50px rgba(0, 0, 0, 0.1);
  }
}
@keyframes warning {
  0% {
    box-shadow: 0 0 15px rgba(0, 0, 0, 0.5), inset 0 0 50px rgba(0, 0, 0, 0.1);
  }
  50% {
    box-shadow: 0 0 25px #ff0000, inset 0 0 50px rgba(0, 0, 0, 0.1);
  }
  100% {
    box-shadow: 0 0 15px rgba(0, 0, 0, 0.5), inset 0 0 50px rgba(0, 0, 0, 0.1);
  }
}
html {
  box-sizing: border-box;
  width: 100%;
  font-family: 'Verdana', sans-serif;
  font-size: 13px;
  line-height: 20px;
}
body {
  width: 100%;
  height: 100%;
  margin: 0;
  background-color: rgba(106, 86, 72, 0.4);
  background-position: center center;
  background-size: cover;
  background-attachment: fixed;
  /* FIXED FOR IE */
}
.isMobile body {
  background: none;
}
.isMobile #bgMobile {
  display: block;
  position: fixed;
  width: 100%;
  height: 100%;
  padding-bottom: 150px;
  -webkit-transform: translateZ(0);
}
.isMobile #bgMobile div {
  position: absolute;
  width: 100%;
  height: 100%;
  background-position: center center;
  background-size: cover;
  background-attachment: fixed;
  /* FIXED FOR IE */
  background-attachment: scroll;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
}
.isMobile body:not(.visual-bonieuwsbrief) #bgMobile div {
  background-position: center center;
  background-size: cover;
  background-attachment: fixed;
  /* FIXED FOR IE */
  background-attachment: scroll;
}
main {
  box-sizing: border-box;
  position: relative;
  z-index: 10;
  height: auto;
  padding-top: 35px;
  overflow: hidden;
  -webkit-transition: all 0.4s 0.1s;
  transition: all 0.4s 0.1s;
}
main {
  top: 100px;
}
html.narrow main {
  top: 80px;
}
.centerStage {
  box-sizing: border-box;
  position: relative;
  width: 100%;
  min-width: 320px;
  max-width: 690px;
  margin: 0 auto;
  border-bottom: 1px solid transparent;
  /* make margins and paddings not collapse in older iOS */
  padding: 0 20px 20px 20px;
  -webkit-transition: opacity 0.4s 0.1s;
  transition: opacity 0.4s 0.1s;
}
#bgShower {
  position: fixed;
  z-index: 1000;
  bottom: 5px;
  right: 5px;
  background-image: url(images/sprites.png);
  background-repeat: no-repeat;
  background-position: 0 -430px;
  width: 70px;
  height: 41px;
  opacity: 0.5;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}
.narrow #bgShower {
  -webkit-transform: scale(0.7);
  transform: scale(0.7);
}
#bgShower:hover,
html.showBackground #bgShower {
  opacity: 1;
  -webkit-transform: scale(1.2);
  transform: scale(1.2);
}
.showBackground header,
.showBackground #calOpener,
.showBackground main,
.showBackground .centerStage,
.showBackground #subMenu {
  opacity: 0;
}
#bgCaption {
  position: fixed;
  z-index: 1000;
  top: 0;
  left: 0;
  opacity: 0;
}
#bgCaption div {
  position: absolute;
  bottom: 20px;
  left: 20px;
  padding-right: 110px;
  font-size: 24px;
  line-height: 30px;
  color: white;
  text-shadow: 0 0 2px #666;
}
#bgCaption div:before,
#bgCaption div:after {
  content: '"';
}
html #bgCaption {
  width: 0;
  height: 0;
  opacity: 0;
  -webkit-transition: opacity 0.5s 0s;
  transition: opacity 0.5s 0s;
}
html.showBackground #bgCaption {
  width: 100%;
  height: 100%;
  opacity: 1;
  -webkit-transition: opacity 3s 2s;
  transition: opacity 3s 2s;
}
button.form-cancel {
  float: left;
  margin: 10px 0 0 20px;
  font-size: 18px;
}
button.form-ok {
  float: right;
  margin: 10px 20px 0 0;
  font-size: 18px;
}
html.inModalMode header,
html.inModalMode #subMenuOpener,
html.inModalMode #subMenu,
html.inModalMode #searchOpener,
html.inModalMode #calOpener,
html.inModalMode main,
html.inModalMode #bgShower,
html.inModalMode #megaPhone {
  display: none;
  opacity: 0;
  visibility: hidden;
}
html.inModalMode.modalAnimating header,
html.inModalMode.modalAnimating #subMenuOpener,
html.inModalMode.modalAnimating #subMenu,
html.inModalMode.modalAnimating #searchOpener,
html.inModalMode.modalAnimating #calOpener,
html.inModalMode.modalAnimating main,
html.inModalMode.modalAnimating #bgShower,
html.inModalMode.modalAnimating #megaPhone {
  display: block;
}
html.inModalMode.showSubMenu #subMenuOpener,
html.inModalMode.showSearch #searchOpener,
html.inModalMode.showCalendar #calOpener {
  display: block;
  opacity: 1;
  visibility: visible;
}
.isOpera .article.style-sticky blockquote {
  background-color: #dcdc8f;
}
.isOpera .article.style-balloon blockquote {
  background-color: white;
}
*[contentEditable='false'] {
  -webkit-user-modify: read-only !important;
}
body header:before,
body #subMenu .menu:before {
  background-color: rgba(55, 90, 55, 0.3);
}
body,
.isMobile body #bgMobile div,
body header,
body #subMenu .menu {
  background-color: rgba(255, 255, 255, 0.4);
}
body nav a:hover {
  background: rgba(255, 255, 255, 0.35);
}
body nav a.active {
  background: rgba(255, 255, 255, 0.5);
}
input[name='user_name'],
input[name='user_email'] {
  margin: 0.5em 0;
  padding: 2px 5px;
  border: solid 1px grey;
}
input[name='user_name']:valid:focus,
input[name='user_email']:valid:focus {
  border-color: green;
}
input[name='user_name']:invalid:focus,
input[name='user_email']:invalid:focus {
  border-color: red;
}
